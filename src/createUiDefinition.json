        },
        "bladeTitle": "Studio & Explorer & Dashboard",
        "elements": [
          {
            "name": "studioSection",
            "type": "Microsoft.Common.Section",
            "label": "Studio",
            "elements": [
              {
                "name": "studio",
                "type": "Microsoft.Common.OptionsGroup",
                "label": "Install Studio?",
                "defaultValue": "Yes",
                "toolTip": "Yes, to provision a single Studio instance.",
                "constraints": {
                  "allowedValues": [

              {
                "name": "vmSizeStorageNodes",
                "type": "Microsoft.Compute.SizeSelector",
                "label": "Storage node VM size",
                "toolTip": "Choose VM SKU size for storage nodes",
                "recommendedSizes": [
                  "Standard_D2s_v4",
                  "Standard_D4s_v4",
                  "Standard_D8s_v4"
                ],
                "constraints": {
                  "allowedSizes": [
                    "Standard_D2as_v4",
                    "Standard_D4as_v4",
                    "Standard_D8as_v4",
                    "Standard_D16as_v4",
                    "Standard_D32as_v4",
                    "Standard_D48as_v4",
                    "Standard_D64as_v4",
                    "Standard_D96as_v4",
                    "Standard_F2s_v2",
                    "Standard_F4s_v2",
                    "Standard_F8s_v2",
                    "Standard_F16s_v2",
                    "Standard_F32s_v2",
                    "Standard_F64s_v2"
                  ]
                },
                "osPlatform": "Linux",
                "count": "[steps('nodesStep').storageNodes.vmStorageNodeCount]"
              }
            ]
          },
          {
            "name": "storageNodesDisks",
            "type": "Microsoft.Common.Section",
            "label": "Storage node disks",
            "elements": [
              {
                "name": "vmStorageDiskSize",
                "type": "Microsoft.Common.DropDown",
                "label": "Size of each managed disk",
                "defaultValue": "16GiB",
                "toolTip": "The size of each data disk to attach to each storage node, it is used for storing log and data",
